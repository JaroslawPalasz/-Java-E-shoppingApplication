<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.lab.controller.BasketController">
   <children>
      <Button fx:id="backButton" layoutX="11.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="Back" />
